#include<stdio.h>
#include<string.h>
int main()
{
	int t,n,i,j,flag=0;
	int a[89];
	scanf("%d",&t);
	while(t--)
	{
		printf(" enter the no:of digits:");
		scanf("%d",&n);
		for(i=0;i<n;i++)
		{
			scanf("%d",&a[i]);
			
		}
		for(i=0,j=n-1;i<=(n-1)/2;i++,j--)
	{
		if(a[i]!=a[j])
		{
			flag=1;
			break;
		}
		}	
		if(flag==1)
		printf("no");
		else
		printf("yes");
		
	}
	return 0;
}
