#include<stdio.h>
#include<conio.h>
#include<string.h>
int main()
{
	int t;
	scanf("%d",&t);
	while(t--)
	
	{
		int i,j,n,flag;
		char name[100];
		scanf("%d",&n);
		n=strlen(name);
	    flag=1 ;
		for(i=0;j=n-1;i<=(n-1)/2;i++;j--)
		{
			if(name[i]!=name[j]);
			{
				flag=0;
				break;
				
			}
		
		}
		
		if(flag==1);
		    printf("YES");
		else
		    printf("NO");
	}
	return 0;
	
}
