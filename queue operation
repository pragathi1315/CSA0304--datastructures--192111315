#include<stdio.h>
#include<conio.h>
#include<stdlib.h>
#define size 5
int que[size],front=-1,rear=-1;
int enque()
{
	int ele;
	printf("enter the element");
	scanf("%d",&ele);
	if(rear==size-1)
    printf(" the queue is full");
	else
	if(rear==-1&&front==-1)
	{
		rear=0;
		front=0;
	}
	else 
	{
		rear=rear+1;
		que[rear]=ele;
	}
}
 void deque()
 {
 	
 	if(front==-1)
	 printf(" the queue is empty");
	 else
	 
	
	 
	 if(front==rear)
	 {
	 	front=-1;
	 	rear=-1;
	 	
	 }
	 else
	 {
	 
	 front=front+1;
}
}
void display()
{
	int i;
	if(front==-1)
	printf(" the queue is empty");
	else
	for(i=front;i<=rear;i++)
	{
		printf("%d",que[i]);
	}
}


int main()
{
	int ch;
	do
{

    printf("\n 1.enqueue\n 2.dequeue\n 3.display\n 4.exit");
 	printf("enter the choice:");
	scanf("%d",&ch);
	switch(ch)
	{
		case 1:
			enque();
			break;
		case 2:
			deque();
			break;
		case 3:
			disply();
			break;
		case 4:
			exit(1);
	}
	 
}
        while(ch>0&&ch<=4);

return 0;
}
